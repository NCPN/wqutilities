' ---------------------------------
' SUB:          Form_Dirty
' Description:
' Parameters:   -
' Returns:      -
' Throws:       -
' References:   -
' Source/date:  John Boetsch - NCCN Landbirds db
' Adapted:      Bonnie Campbell, May 2014 for NCPN WQ Utilities tool
' Revisions:    BLC, 6/19/2014 - Replaced cAppMode with TempVars.Item("UserAccessLevel")
' ---------------------------------
Private Sub Form_Dirty(Cancel As Integer)
    On Error GoTo Err_Handler

    ' Note: this event is ignored on inserting a new record if BeforeInsert code exists

    ' Check if the current record is certified
    If IsNull(Me.Certified_date) = False And (IsNull(Me.Updated_date) Or _
        Me.Certified_date >= Me.Updated_date) Then
        If TempVars.item("UserAccessLevel") = "data entry" Or _
            IsNull(TempVars.item("UserAccessLevel")) Then
        ' For data entry applications, warn the user and disallow edits
            MsgBox "Edits to certified event data are not allowed in data entry mode", vbOKOnly _
                + vbCritical, "This event record has been certified"
            DoCmd.CancelEvent
            GoTo Exit_Procedure
        Else
        ' For other application modes, request confirmation before allowing edits
            If MsgBox("This record is certified ... are you certain you want to edit it?", _
                vbYesNo + vbExclamation + vbDefaultButton2, "Confirm certified data edit") = vbNo Then
                DoCmd.CancelEvent
                GoTo Exit_Procedure
            End If
            Me.cmdUndo.Visible = True
            MsgBox "Please log the edits to certified data ..."
            DoCmd.OpenForm "frm_Edit_Log", , , , , , "Update tbl_Rare_Bird_Obs"
        End If
    Else
        Me.cmdUndo.Visible = True
    End If

Exit_Procedure:
    Exit Sub

Err_Handler:
    MsgBox "Error #" & Err.Number & ": " & Err.Description, vbCritical
    Resume Exit_Procedure

End Sub

' ---------------------------------
' SUB:          Form_BeforeUpdate
' Description:
' Parameters:   -
' Returns:      -
' Throws:       -
' References:   -
' Source/date:  John R. Boetsch, May 2008
' Adapted:      Bonnie Campbell, June, 2014 for NCPN WQ Utilities tool
' Revisions:    BLC, 7/29/2014 - XX
' ---------------------------------
Private Sub Form_BeforeUpdate(Cancel As Integer)
    On Error GoTo Err_Handler

    ' Validate and complete the record prior to saving
    If IsNull(Me.txtDate_established) Then
        MsgBox "Please enter the date", vbOKOnly, "Validation error"
        Me.txtDate_established.SetFocus
        DoCmd.CancelEvent
        GoTo Exit_Procedure
    ElseIf IsNull(Me.txtStart_time) Then
        MsgBox "Please enter the observation time (enter '0:00' if unknown)", _
            vbOKOnly, "Validation error"
        Me.txtStart_time.SetFocus
        DoCmd.CancelEvent
        GoTo Exit_Procedure
    ElseIf IsNull(Me.cmbObserver) And Me.cmbObserver.Visible Then
        MsgBox "Please enter the observer name", vbOKOnly, "Validation error"
        Me.cmbObserver.SetFocus
        DoCmd.CancelEvent
        GoTo Exit_Procedure
    ElseIf IsNull(Me.cmbTaxon_ID) Then
        MsgBox "Please enter the species observed", vbOKOnly, "Validation error"
        Me.cmbTaxon_ID.SetFocus
        DoCmd.CancelEvent
        GoTo Exit_Procedure
    ElseIf IsNull(Me.txtGroup_size) Then
        MsgBox "Please enter the group size", vbOKOnly, "Validation error"
        Me.txtGroup_size.SetFocus
        DoCmd.CancelEvent
        GoTo Exit_Procedure
    ElseIf IsNull(Me.cmbNest_activity) Then
        MsgBox "Missing a nesting activity", vbOKOnly, "Validation error"
        Me.cmbNest_activity.SetFocus
        DoCmd.CancelEvent
        GoTo Exit_Procedure
    End If
    ' Soft validation rules:
    If IsNull(Me.txtLocation_name) Then
        If MsgBox("Missing a location name ... would you like to enter one?", _
            vbYesNo + vbDefaultButton2, "Missing data") = vbYes Then
            Me.txtLocation_name.SetFocus
            DoCmd.CancelEvent
            GoTo Exit_Procedure
        End If
    End If

    ' Complete the record
    If IsNull(Me.Park_code) Then Me.Park_code = Me.cmbPark
    If Me.NewRecord Then
        If IsNull(Me.Location_code) Then Me.Location_code = "Rare"
        If IsNull(Me.Location_type) Then Me.Location_type = "Incidental"
        If IsNull(Me.Location_status) Then Me.Location_status = "Incidental"
        If IsNull(Me.Start_date) Then Me.Start_date = Me.txtDate_established
    End If
    If IsNull(Me.Declination) And IsNull(varDeclination) = False Then _
        Me.Declination = varDeclination

    ' Update the location entered/updated info
    Me.Loc_updated_by = strCurrentUser
    If Me.NewRecord = False Then Me.Loc_updated = Now()
    ' Prior to saving, update the event entered/updated info
    If Me.NewRecord Then
        Me.Entered_by = strCurrentUser
    Else
        Me.Updated_by = strCurrentUser
        Me.Updated_date = Now()
    End If

Exit_Procedure:
    Exit Sub

Err_Handler:
    MsgBox "Error #" & Err.Number & ": " & Err.Description, vbCritical
    Resume Exit_Procedure

End Sub

' ---------------------------------
' SUB:          Form_AfterUpdate
' Description:
' Parameters:   -
' Returns:      -
' Throws:       -
' References:   -
' Source/date:  John R. Boetsch, May 2008
' Adapted:      Bonnie Campbell, June, 2014 for NCPN WQ Utilities tool
' Revisions:    BLC, 7/29/2014 - XX
' ---------------------------------
Private Sub Form_AfterUpdate()
    On Error GoTo Err_Handler

    Dim strSQL As String

    Me.cmdUndo.Visible = False

    ' Insert the observer record if not already present
    If IsNull(Me.cmbObserver) = False Then
        If DCount("*", "tbl_Observers", "[Event_ID] = """ & Me.Event_ID & _
            """ AND [Observer_role] = """ & Me.txtObserverRole & """ AND [Contact_ID] = """ _
            & Me.cmbObserver & """") = 0 Then
            ' Insert the observer record if none exists
            strSQL = "INSERT INTO tbl_Observers ( Event_ID, Contact_ID, Observer_role ) SELECT """ _
                & Me.Event_ID & """ AS Event_ID, """ & Me.cmbObserver & """ AS Contact_ID, """ _
                & Me.txtObserverRole & """ AS Observer_role;"
            DoCmd.SetWarnings False
            DoCmd.RunSQL strSQL
            DoCmd.SetWarnings True
        End If
    End If
    ' Repeat insert for second observer, if any
    If IsNull(Me.cmbObserver2) = False Then
        If DCount("*", "tbl_Observers", "[Event_ID] = """ & Me.Event_ID & _
            """ AND [Observer_role] = """ & Me.txtObserverRole & """ AND [Contact_ID] = """ _
            & Me.cmbObserver2 & """") = 0 Then
            ' Insert the observer record if none exists
            strSQL = "INSERT INTO tbl_Observers ( Event_ID, Contact_ID, Observer_role ) SELECT """ _
                & Me.Event_ID & """ AS Event_ID, """ & Me.cmbObserver2 & """ AS Contact_ID, """ _
                & Me.txtObserverRole & """ AS Observer_role;"
            DoCmd.SetWarnings False
            DoCmd.RunSQL strSQL
            DoCmd.SetWarnings True
        End If
    End If

Exit_Procedure:
    DoCmd.SetWarnings True
    Exit Sub

Err_Handler:
    MsgBox "Error #" & Err.Number & ": " & Err.Description, vbCritical
    Resume Exit_Procedure

End Sub

