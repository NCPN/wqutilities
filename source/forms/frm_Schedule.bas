' ---------------------------------
' SUB:          cmdAutoPopulate_Click
' Description:
' Parameters:   -
' Returns:      -
' Throws:       -
' References:   -
' Source/date:  John R. Boetsch, May 2008
' Adapted:      Bonnie Campbell, June, 2014 for NCPN WQ Utilities tool
' Revisions:    BLC, 7/29/2014 - XX
' ---------------------------------
Private Sub cmdAutoPopulate_Click()
    On Error GoTo Err_Handler

    Dim strSQL As String
    Dim strYearFilter As String
    Dim varYear As Variant
    Dim varPanel As Variant
    Dim varPark As Variant
    Dim intI As Integer

    If MsgBox("This will automatically add records to the sampling schedule." & _
        vbCrLf & "Sites with a status of 'Active' or 'Proposed' will be added" & _
        vbCrLf & "from the annual panel plus the alternating panel you specify." & vbCrLf & _
        vbCrLf & "You will be prompted for the following information:" & vbCrLf & _
        vbCrLf & "1) Sampling year" & vbCrLf & "2) Alternating panel name (e.g., 2-6)" & _
        vbCrLf & "3) Small park to be sampled (e.g., LEWI or SAJH, or neither)" & _
        vbCrLf & vbCrLf & "Other sites may be added individually as needed.", _
        vbOKCancel, "Auto populate sample schedule?") = vbCancel Then GoTo Exit_Procedure

    ' Get an initial count of records already in the schedule table
    intI = DCount("*", "tbl_Schedule")

    If Not IsNull(Me.cmbYearFilter) Then strYearFilter = Me.cmbYearFilter

Enter_year:
    varYear = Trim(InputBox("Enter the sampling year to be added to the schedule:", _
        "Schedule year", strYearFilter))
    If IsNull(varYear) Or varYear = Empty Then
        MsgBox "No records added"
        GoTo Exit_Procedure
    ElseIf Len(varYear) <> 4 Or IsNumeric(varYear) = False Then
        MsgBox "You must enter a year in YYYY format"
        GoTo Enter_year
    ElseIf CInt(varYear) < 2000 Or CInt(varYear) > 2100 Then
        MsgBox "Please enter a year between 2000 and 2100"
        GoTo Enter_year
    End If

Enter_panel:
    varPanel = Trim(InputBox("Enter the alternating panel number (2-6):", _
        "Alternating panel to be visited"))
    If IsNull(varPanel) Or varPanel = Empty Then
        MsgBox "No records added"
        GoTo Exit_Procedure
    ElseIf Len(varPanel) > 1 Or IsNumeric(varPanel) = False Then
        MsgBox "You must enter a numeric panel number"
        GoTo Enter_panel
    ElseIf CInt(varPanel) < 2 Or CInt(varPanel) > 6 Then
        MsgBox "Please enter a panel number 2 and 6"
        GoTo Enter_panel
    End If

Enter_park:
    varPark = Trim(InputBox("Enter the small park code ('SAJH' or 'LEWI' or 'none'):", _
        "Small park to be visited"))
    If IsNull(varPark) Or varPark = Empty Then
        MsgBox "No records added"
        GoTo Exit_Procedure
    ElseIf varPark <> "SAJH" And varPark <> "LEWI" And varPark <> "none" Then
        MsgBox "You must enter either 'SAJH', 'LEWI' or 'none'"
        GoTo Enter_park
    End If

    strSQL = "INSERT INTO tbl_Schedule ( Calendar_year, Site_ID ) " & _
        "SELECT """ & varYear & """ AS Calendar_year, tbl_Sites.Site_ID " & _
        "FROM tbl_Sites " & _
        "WHERE (((tbl_Sites.Panel_name)='1' Or (tbl_Sites.Panel_name)=""" & varPanel & _
        """) AND ((tbl_Sites.Site_status)='Active' Or (tbl_Sites.Site_status)='Proposed')) " & _
        "OR (((tbl_Sites.Park_code)=""" & varPark & """) AND " & _
        "((tbl_Sites.Site_status)='Active' Or (tbl_Sites.Site_status)='Proposed'));"

    ' Run the SQL statement to append the records
    DoCmd.SetWarnings False
    DoCmd.RunSQL strSQL
    DoCmd.SetWarnings True

    ' Revise the count and let the user know
    intI = DCount("*", "tbl_Schedule") - intI
    MsgBox intI & " records were added to the sampling schedule"

    ' Requery the form
    Me.cmbYearFilter = varYear
    Me.subSchedule.Form.Filter = "[Calendar_year]=""" & Me.cmbYearFilter & """"
    Me.subSchedule.Form.FilterOn = True

Exit_Procedure:
    On Error Resume Next
    DoCmd.SetWarnings True
    Exit Sub

Err_Handler:
    Select Case Err.Number
      Case 2001 ' canceled event, do nothing
      Case Else
        MsgBox "Error #" & Err.Number & ": " & Err.Description, vbCritical
    End Select
    Resume Exit_Procedure

End Sub