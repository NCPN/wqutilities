' ---------------------------------
' SUB:     cmdDataEntry_Click
' Description:
' Parameters:   -
' Returns:      -
' Throws:       -
' References:   -
' Source/date:  John R. Boetsch, May 2008
' Adapted:      Bonnie Campbell, June, 2014 for NCPN WQ Utilities tool
' Revisions:    BLC, 6/13/2014 - XX
' ---------------------------------
Private Sub cmdDataEntry_Click()
    On Error GoTo Err_Handler

    Dim varLoc As Variant
    Dim varEvent As Variant
    Dim bResetFilter As Boolean

    ' Bail out if no location ID
    If IsNull(Me.Location_ID) Then GoTo Exit_Procedure

    ' Set the global reference variables
    Set gvarRefForm = Me.Form
    Set gvarRefCtl = Me.txtLoc_code

    Select Case Me.Loc_type
      Case "Transect origin"
        If IsNull(Me.Event_ID) Then
            ' Filter by location alone if no event exists
            DoCmd.OpenForm "frm_Data_Entry", , , , acFormAdd, , Me.Location_ID
        Else
            ' Filter by location and event
            DoCmd.OpenForm "frm_Data_Entry", , , "[Event_ID]=""" & Me.Event_ID & _
                """", , , Me.Location_ID
        End If

      Case "Sample point"
        ' A new or existing sampling point .. must find the related origin record and
        '   set the open form filter to that record
        If Me.togFilterByType And Me.cmbTypeFilter = "Sample point" Then
            ' If the form is filtered on sample point, the origin record won't be
            '   found, so must first turn off the filter
            Me.togFilterByType = False
            fxnFilterRecords
            bResetFilter = True
        Else: bResetFilter = False
        End If
        ' Move to the related record
        Me.RecordsetClone.FindFirst "[Site_ID] = """ & Me.Site_ID & _
            """ AND [Loc_type] = 'Transect origin'"
        If Me.RecordsetClone.NoMatch = True Then
            If MsgBox("No transect events match this point sample date." & vbCrLf & _
                vbCrLf & "Would you like to open the point sampling form instead?", _
                vbYesNo + vbExclamation, "No matching transect visit") = vbYes Then
                If IsNull(Me.Event_ID) Then
                    DoCmd.OpenForm _
                        "frm_Point_Establishment", , , , , , "LocID=" & Me.Location_ID
                Else
                    DoCmd.OpenForm _
                        "frm_Point_Establishment", , , , , , "Event=" & Me.Event_ID
                End If
            End If
            GoTo Exit_Procedure
        End If
        varLoc = Me.RecordsetClone.Location_ID
        varEvent = Me.RecordsetClone.Event_ID
        ' If the filter was turned off, turn it back on
        If bResetFilter Then
            Me.togFilterByType = True
            fxnFilterRecords
        End If
        
        If IsNull(varEvent) Then
        ' Filter by location alone if no event exists
            DoCmd.OpenForm "frm_Data_Entry", , , , acFormAdd, , varLoc
        Else
        ' Filter by location and event
            DoCmd.OpenForm "frm_Data_Entry", , , "[Event_ID]=""" & varEvent & _
                """", , , varLoc
        End If

      Case "Rare bird obs"
        ' Open the rare bird observation data entry form
        If IsNull(Me.Event_ID) Then
            ' Filter by location alone if no event exists
            DoCmd.OpenForm "frm_Rare_Bird_Obs", , , "[Location_ID]=""" & Me.Location_ID & _
                """", , , Me.Park_code
        Else
            ' Filter by location and event
            DoCmd.OpenForm "frm_Rare_Bird_Obs", , , "[Event_ID]=""" & Me.Event_ID & _
                """", , , Me.Park_code
        End If

      Case Else
            ' Nothing
    End Select

Exit_Procedure:
    Exit Sub

Err_Handler:
    MsgBox "Error #" & Err.Number & ": " & Err.Description, vbCritical
    Resume Exit_Procedure

End Sub

