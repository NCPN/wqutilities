' ---------------------------------
Private Sub optgCertifiedMode_AfterUpdate()
    On Error GoTo Err_Handler

    Dim strMsg As String
    Dim strMsgDataEntry As String

    strMsg = "Certified event records are those that have already passed" & vbCrLf & _
        "the data quality review. Edits to certified data must be" & vbCrLf & _
        "logged in the project edit log." & vbCrLf & vbCrLf & _
        "Are you certain you want to view certified event records?"

    strMsgDataEntry = "Certified event records are not viewable when the" & vbCrLf & _
        "database is in data entry mode."

    If Me.optgCertifiedMode = 1 Then
        Select Case TempVars.item("UserAccessLevel")
          Case "data entry"
            MsgBox strMsgDataEntry, vbOKOnly + vbInformation, _
                "Cannot show certified event data"
            Me.optgCertifiedMode = 0
            GoTo Exit_Procedure
          Case Else
            If MsgBox(strMsg, vbExclamation + vbYesNo + vbDefaultButton2, _
                "Confirm show certified event data") = vbNo Then
                Me.optgCertifiedMode = 0
                GoTo Exit_Procedure
            Else
                ' Make sure year filter controls are enabled
                Me.cmbYearFilter.Locked = False
                Me.togFilterByYear.Enabled = True
            End If
        End Select
    End If
    fxnSetRecSource

Exit_Procedure:
    Exit Sub

Err_Handler:
    MsgBox "Error #" & Err.Number & ": " & Err.Description, vbCritical
    Resume Exit_Procedure

End Sub

