' SUB:     cmdDeleteRec_Click
' Description:
' Parameters:   -
' Returns:      -
' Throws:       -
' References:   -
' Source/date:  John R. Boetsch, May 2008
' Adapted:      Bonnie Campbell, June, 2014 for NCPN WQ Utilities tool
' Revisions:    BLC, 6/13/2014 - XX
' ---------------------------------
Private Sub cmdDeleteRec_Click()
    On Error GoTo Err_Handler

    Dim strSQL As String
    Dim strMsg As String
    Dim varResponse As VbMsgBoxResult

    ' Bail out if in read only mode
    If TempVars.item("UserAccessLevel") = "read only" Then GoTo Exit_Procedure

    If IsNull(Me.Location_ID) = False Then
    ' If there is a location ID in the record ...

        If Me.Loc_type = "Rare bird obs" Then
        ' Confirm deletion of rare bird observation location/event records
            strMsg = "Are you sure you want to delete this rare bird observation?" & _
                vbCrLf & vbCrLf & Me.Loc_code & " sampled on " & Me.Start_date
            varResponse = MsgBox(strMsg, vbYesNo + vbDefaultButton2 + vbExclamation, _
                "Delete the sampling event?")
            Select Case varResponse
              Case vbYes
                ' Extra confirmation in case of certified event data
                If Me.Rec_status = "Certified" Then
                    If TempVars.item("UserAccessLevel") = "data entry" Then
                    ' Bail out - not allowed
                        MsgBox "Certified records may not be deleted in data entry mode.", , _
                            "Cannot delete this location"
                        GoTo Exit_Procedure
                    Else
                        If MsgBox("This is a certified record ..." & vbCrLf & vbCrLf & _
                            "Please log the deletion in the Edit Log." & vbCrLf & vbCrLf _
                            & Me.Loc_code & " sampled on " & Me.Start_date & vbCrLf & vbCrLf _
                            & "(Also, this is your last chance to cancel the delete!)", _
                            vbCritical + vbOKCancel + vbDefaultButton2, _
                            "Confirm Delete Certified Record") = vbCancel Then
                            GoTo Exit_Procedure
                        Else
                            DoCmd.OpenForm "frm_Edit_Log", , , , , , "Delete tbl_Events"
                        End If
                    End If
                End If
                ' Delete the sampling location (and all down-stream records)
                strSQL = "DELETE * FROM tbl_Locations WHERE ((tbl_Locations.Location_ID) = """ _
                    & Me.Location_ID & """)"

              Case Else
                GoTo Exit_Procedure
            End Select

        ElseIf IsNull(Me.Start_date) Then
        ' If there is no event associated, confirm deleting the location record
            If Me.Loc_type = "Transect origin" Or Me.Loc_type = "Sample point" Then
                If TempVars("UserAccessLevel") = "data entry" Then
                ' Bail out - these types cannot be deleted in data entry mode
                    MsgBox "Transect origins and established sample points" & vbCrLf & _
                        "may not be deleted in data entry mode.", , _
                        "Cannot delete this location"
                    GoTo Exit_Procedure
                Else
                    strMsg = "This record is an established " & Me.Loc_type & "!" & _
                        vbCrLf & vbCrLf & "You are about to delete the following location:" & _
                        vbCrLf & vbCrLf & Me.Loc_code & _
                        vbCrLf & vbCrLf & "(Note: the record may not be deleted if related" & _
                        vbCrLf & "records exist in certain tables)"
                    varResponse = MsgBox(strMsg, vbYesNo + vbDefaultButton2 + vbCritical, _
                        "WARNING: Delete the established sample location?")
                End If
            Else    ' ... a newly-entered sample point
                strMsg = "You are about to delete the following location:" & _
                    vbCrLf & vbCrLf & Me.Loc_code & " (type: " & Me.Loc_type & ")" & _
                    vbCrLf & vbCrLf & "(Note: the record may not be deleted if related" & _
                    vbCrLf & "records exist in certain tables)"
                varResponse = MsgBox(strMsg, vbYesNo + vbDefaultButton2 + vbCritical, _
                    "WARNING: Delete the sample location?")
            End If

            Select Case varResponse
              Case vbYes
                ' Build the statement to delete the sampling location
                '   (and all down-stream records)
                strSQL = "DELETE * FROM tbl_Locations " & _
                    "WHERE ((tbl_Locations.Location_ID) = """ _
                    & Me.Location_ID & """)"

              Case Else
                GoTo Exit_Procedure
            End Select

        Else
        ' Ask whether or not to delete the sampling event data
            strMsg = "You are about to delete the sampling event data for the " & _
                vbCrLf & "following sample location and date:" & _
                vbCrLf & vbCrLf & Me.Loc_code & " sampled on " & Me.Start_date
            varResponse = MsgBox(strMsg, vbYesNo + vbDefaultButton2 + vbExclamation, _
                "Delete the sampling event?")
            Select Case varResponse
              Case vbYes
                ' Extra confirmation in case of certified event data
                If Me.Rec_status = "Certified" Then
                    If TempVars.item("UserAccessLevel") = "data entry" Then
                    ' Bail out - not allowed
                        MsgBox "Certified records may not be deleted in data entry mode.", , _
                            "Cannot delete this location"
                        GoTo Exit_Procedure
                    Else
                        If MsgBox("This is a certified record ..." & vbCrLf & vbCrLf & _
                            "Please log the deletion in the Edit Log." & vbCrLf & vbCrLf _
                            & Me.Loc_code & " sampled on " & Me.Start_date & vbCrLf & vbCrLf _
                            & "(Also, this is your last chance to cancel the delete!)", _
                            vbCritical + vbOKCancel + vbDefaultButton2, _
                            "Confirm Delete Certified Record") = vbCancel Then
                            GoTo Exit_Procedure
                        Else
                            DoCmd.OpenForm "frm_Edit_Log", , , , , , "Delete tbl_Events"
                        End If
                    End If
                End If
                ' Build the statement to delete the sampling event
                '   (and all down-stream records)
                strSQL = "DELETE * FROM tbl_Events " & _
                    "WHERE ((tbl_Events.Event_ID) = """ _
                    & Me.Event_ID & """)"

              Case Else
                GoTo Exit_Procedure
            End Select
        End If

        DoCmd.SetWarnings False
        DoCmd.RunSQL strSQL
        DoCmd.SetWarnings True
        Me.Requery

    Else
        ' If there is no location associated, confirm deleting the location record
        MsgBox "Select a location record to delete"
    End If

Exit_Procedure:
    On Error Resume Next
    DoCmd.SetWarnings True
    Exit Sub

Err_Handler:
    Select Case Err.Number
      Case 2501   ' Canceled RunSQL command
        MsgBox "The record was not deleted"
      Case Else
        MsgBox "Error #" & Err.Number & ": " & Err.Description, vbCritical
    End Select
    Resume Exit_Procedure

End Sub

