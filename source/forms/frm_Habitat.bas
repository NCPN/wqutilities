' =================================
' FORM NAME:    frm_Habitat
' Description:  Data entry form for habitat description data
' Data source:  In-line record source query based on Locations and Events tables
' Data access:  edit only
' Pages:        none
' Functions:    fxnInsertObserver
' References:   fxnSwitchboardIsOpen
' Source/date:  John R. Boetsch, July 2006
' Revisions:    JRB, 6/11/2008 - eliminated some unnecessary code (re: edits only related to observer);
'                   added confirmation step before allowing edits to a certified record
'               JRB, 7/9/2008 - updated certified data check syntax
'               JRB, 10/15/2008 - updated gvarRefForm/Ctl error code in cmdClose
' =================================

Dim strCurrentUser As String
Dim varEvent As Variant

' ---------------------------------
' SUB:          Form_Open
' Description:
' Parameters:   -
' Returns:      -
' Throws:       -
' References:   -
' Source/date:  John R. Boetsch, May 2008
' Adapted:      Bonnie Campbell, June, 2014 for NCPN WQ Utilities tool
' Revisions:    BLC, 7/29/2014 - updated to use TempVars.Item("User") vs. cUser
' ---------------------------------
Private Sub Form_Open(Cancel As Integer)
    On Error GoTo Err_Handler

    ' Note: fewer opening items because this form is not for creating
    '   new event records, only for navigating to existing ones

    ' Set the event variable to the transect visit event (i.e., not the sampling point event)
    '   Note: this will be different from Me.Event_ID when the form is set to a sampling point event
    If Me.OpenArgs <> "" Then
        varEvent = Me.OpenArgs
    Else
        varEvent = Null
        MsgBox "There is no transect visit event for this form", , "Form open error"
        DoCmd.CancelEvent
        GoTo Exit_Procedure
    End If

    ' Set the current user
    If fxnSwitchboardIsOpen Then
        If IsNull(TempVars.item("User")) = False Then
            strCurrentUser = TempVars.item("User")
        Else
            strCurrentUser = Environ("Username")
        End If
    Else
        strCurrentUser = Environ("Username")
    End If

Exit_Procedure:
    Exit Sub

Err_Handler:
    MsgBox "Error #" & Err.Number & ": " & Err.Description, vbCritical
    Resume Exit_Procedure

End Sub

' ---------------------------------
' SUB:          Form_Current
' Description:
' Parameters:   -
' Returns:      -
' Throws:       -
' References:   -
' Source/date:  John R. Boetsch, May 2008
' Adapted:      Bonnie Campbell, June, 2014 for NCPN WQ Utilities tool
' Revisions:    BLC, 7/29/2014 - updated to use TempVars.Item("Park") vs. cPark
' ---------------------------------
Private Sub Form_Current()
    On Error GoTo Err_Handler

    Dim varObserver As Variant

    If Me.NewRecord = False Then
        Me.AllowAdditions = False
        ' On existing records, lock the unbound header controls and load data
        Me.cmbPark.Locked = True
        Me.cmbSite.Locked = True
        Me.txtDate.Locked = True
        Me.cmbPark = Me.Park_code
        Me.cmbSite = Me.Site_ID
        Me.txtDate = Me.Start_date
        Me.cmbPointEvent.Enabled = True

        ' Load the point event and observer if the current event is not the transect event
        If varEvent <> Me.Event_ID Then
            ' Set and lock the point event selector
            Me.cmbPointEvent = Me.Event_ID
            Me.cmbPointEvent.Locked = True
            ' Enable the remaining form controls for data entry
            Me.subHabitatData.Enabled = True
            Me.cmdNextPoint.Enabled = True
            ' Load the observer if there is one
            varObserver = DLookup("Contact_ID", "tbl_Observers", "[Event_ID] = """ & _
                Me.Event_ID & """ AND [Observer_role] = """ & Me.txtObserverRole & """")
            If (IsNull(Me.cmbObserver) And IsNull(varObserver) = False) _
                Or (IsNull(Me.cmbObserver) = False And IsNull(varObserver) = False And _
                Me.cmbObserver <> varObserver) Then
                ' The observer for the current record is different than what is in the control
                Me.cmbObserver = varObserver
            ElseIf IsNull(varObserver) And IsNull(Me.cmbObserver) = False Then
                ' No observer in the record, but one in the control
            '    If MsgBox("Would you like to keep the same observer for this event?", _
            '        vbYesNo, "Add observer to this record") = vbYes Then
                    fxnInsertObserver (Me.cmbObserver)
            '    Else
            '        Me.cmbObserver = Null
            '    End If
            End If
            ' Unlock the observer control if it is null
            Me.cmbObserver.Locked = Not IsNull(Me.cmbObserver)
            ' Set the focus to the observer if blank, else the subform
            If IsNull(Me.cmbObserver) Then
                Me.cmbObserver.SetFocus
            Else: Me.subHabitatData.SetFocus
            End If

        Else    ' The form is currently set to the transect visit event instead of a sample event
            ' Disable the data controls and blank out the point selector
            Me.cmbPointEvent.Locked = False
            Me.cmbPointEvent = Null
            Me.cmbObserver.Locked = False
            If IsNull(Me.cmbObserver) Then Me.cmbObserver.SetFocus Else Me.cmbPointEvent.SetFocus
            ' Disable the data entry form controls
            Me.subHabitatData.Enabled = False
            Me.cmdNextPoint.Enabled = False
        End If

    Else
        ' Note: the following code should not need to run as the form should always be pointed
        '   to an existing record
        If Me.AllowAdditions = False Then Me.AllowAdditions = True
        ' On new records, unlock and blank out header controls
        If fxnSwitchboardIsOpen Then Me.cmbPark = TempVars.item("Park")
        Me.cmbSite.Locked = False
        Me.txtDate.Locked = False
        Me.cmbObserver.Locked = False
        Me.cmbSite = Null
        Me.txtDate = Null
        Me.cmbObserver = Null
        Me.cmbSite.SetFocus
        ' Blank out point selector and disable the other form controls
        Me.cmbPointEvent.Enabled = False
        Me.cmbPointEvent = Null
        If Me.subHabitatData.Enabled Then Me.subHabitatData.Enabled = False
        Me.cmdNextPoint.Enabled = False
    End If

Exit_Procedure:
    Exit Sub

Err_Handler:
    MsgBox "Error #" & Err.Number & ": " & Err.Description, vbCritical
    Resume Exit_Procedure

End Sub

' ---------------------------------
' SUB:          Form_Dirty
' Description:
' Parameters:   -
' Returns:      -
' Throws:       -
' References:   -
' Source/date:  John Boetsch - NCCN Landbirds db
' Adapted:      Bonnie Campbell, May 2014 for NCPN WQ Utilities tool
' Revisions:    BLC, 6/19/2014 - Replaced cAppMode with TempVars.Item("UserAccessLevel")
' ---------------------------------
Private Sub Form_Dirty(Cancel As Integer)
    On Error GoTo Err_Handler

    ' Note: this event is ignored on inserting a new record if BeforeInsert code exists

    ' Check if the current record is certified
    If IsNull(Me.Certified_date) = False And (IsNull(Me.Updated_date) Or _
        Me.Certified_date >= Me.Updated_date) Then
        If TempVars.item("UserAccessLevel") = "data entry" Or _
            IsNull(TempVars.item("UserAccessLevel")) Then
        ' For data entry applications, warn the user and disallow edits
            MsgBox "Edits to certified event data are not allowed in data entry mode", vbOKOnly _
                + vbCritical, "This event record has been certified"
            DoCmd.CancelEvent
            GoTo Exit_Procedure
        Else
        ' For other application modes, request confirmation before allowing edits
            If MsgBox("This record is certified ... are you certain you want to edit it?", _
                vbYesNo + vbExclamation + vbDefaultButton2, "Confirm certified data edit") = vbNo Then
                DoCmd.CancelEvent
                GoTo Exit_Procedure
            End If
            MsgBox "Please log the edits to certified data ..."
            DoCmd.OpenForm "frm_Edit_Log", , , , , , "Update tbl_Habitat"
        End If
    End If

Exit_Procedure:
    Exit Sub

Err_Handler:
    MsgBox "Error #" & Err.Number & ": " & Err.Description, vbCritical
    Resume Exit_Procedure

End Sub

