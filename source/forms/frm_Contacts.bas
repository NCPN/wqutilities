' ---------------------------------
' FUNCTION:     fxnFormDefaults
' Description:  Sets properties of the form depending on the form mode
' Parameters:   strFormMode - form mode (view, edit, new)
' Returns:      none
' Throws:       none
' References:   none
' Source/date:  John R. Boetsch, 2002
' Revisions:    JRB, 5/26/2006 - documentation, updated code for enabling/disabling
'               controls
' ---------------------------------
Function fxnFormDefaults(strFormMode As String)
    On Error GoTo Err_Handler

    Dim bLockState As Boolean

    bLockState = True   ' Default is to lock controls

    ' The form should not be reset to view or edit mode if there are no records
    If bNoRecords Then strFormMode = "new"

    ' Change the form properties depending on the mode specified by the user
    Select Case strFormMode
      Case "new"
        ' Modify the form properties to allow new records
        Me.cmdClose.SetFocus    ' Must do this before turning off new button
        Me.cmdUndo.Visible = True
        Me.cmdSubmit.Visible = True
        Me.cmdEdit.Enabled = False
        Me.cmdNew.Enabled = False
        Me.AllowAdditions = True
        Me.Detail.BackColor = 12574431 ' haystack
        DoCmd.GoToRecord , , acNewRec
        Me.txtFirstName.SetFocus    ' Needed on new record before disabling ctls
        ' Unlock fields
        bLockState = False
        GoTo Change_Ctl_State

      Case "edit"
        ' Modify the form properties to allow edits
        Me.cmdClose.SetFocus    ' Must do this before turning off edit button
        Me.cmdUndo.Visible = True
        Me.cmdSubmit.Visible = True
        Me.cmdEdit.Enabled = False
        Me.cmdNew.Enabled = False
        Me.AllowAdditions = True
        Me.Detail.BackColor = 12574431 ' haystack
        ' Unlock fields
        bLockState = False
        GoTo Change_Ctl_State

      Case "view"
        ' Set the form to the default form view
        Me.cmdClose.SetFocus    ' Must do this before disabling ctls
        Me.cmdUndo.Visible = False
        Me.cmdSubmit.Visible = False
        Me.cmdEdit.Enabled = True
        Me.cmdNew.Enabled = True
        Me.AllowAdditions = False
        Me.Detail.BackColor = 14541277 ' light blue (default)
        ' Lock fields
        bLockState = True
        GoTo Change_Ctl_State

    End Select

Change_Ctl_State:
    Me.optgFilterContacts.Locked = Not bLockState
    Me.cmbContact.Enabled = bLockState
    Me.txtFirstName.Locked = bLockState
    Me.txtLastName.Locked = bLockState
    Me.txtMiddleInit.Locked = bLockState
    Me.cmbOrganization.Locked = bLockState
    Me.cmbPosition.Locked = bLockState
    Me.cmbLocation.Locked = bLockState
    Me.txtWorkVoice.Locked = bLockState
    Me.txtWorkExt.Locked = bLockState
    Me.txtEmail.Locked = bLockState
    Me.txtFax.Locked = bLockState
    Me.txtHomeVoice.Locked = bLockState
    Me.txtMobile.Locked = bLockState
    Me.txtNotes.Locked = bLockState
    Me.chkIsActive.Locked = bLockState
    ' This control should always be locked unless unlocked due to a conflict on new records
    Me.txtContactID.Locked = True

Exit_Procedure:
    Exit Function

Err_Handler:
    Select Case Err.Number
      Case Else
        MsgBox "Error #" & Err.Number & ": " & Err.Description, vbCritical, _
            "Error encountered (#" & Err.Number & " - fxnFormDefaults)"
    End Select
    Resume Exit_Procedure

End Function

' ---------------------------------
' FUNCTION:     fxnBuildID
' Description:  Builds the aggregate key value from last name/first name
' Parameters:   none
' Returns:      none
' Throws:       none
' References:   fxnChangeDelimiter
' Source/date:  John R. Boetsch, 2002
' Revisions:    JRB, 5/25/2006 - documentation, added function call to
'                   remove spaces and quotes from contact ID strings
'               JRB, 6/8/2007 - updated to add a user confirmation of new contact ID
'                   and a record undo in case of an empty string
'               JRB, 6/23/2009 - replaced fxnTrimSpaces with Trim()
' ---------------------------------
Function fxnBuildID()
    On Error GoTo Err_Handler

    Dim strContactID As String
    Dim strTemp As String

    ' If the first name and last name have been entered ...
    If IsNull(Me.txtLastName) = False And IsNull(Me.txtFirstName) = False Then
        ' Remove leading or trailing spaces before building
        strTemp = Me.txtLastName
        Me.txtLastName = Trim(strTemp)
        strTemp = Me.txtFirstName
        Me.txtFirstName = Trim(strTemp)

        ' Build the contact ID
        strContactID = Me!Last_name & "_" & Me!First_name
        If IsNull(Me!Middle_init) = False Then
            ' Remove leading or trailing spaces before building
            strTemp = Me.txtMiddleInit
            Me.txtMiddleInit = Trim(strTemp)
            strContactID = strContactID & "_" & Me.txtMiddleInit
        End If

        ' Clean up any quotation marks in the expression
        strContactID = fxnChangeDelimiter(strContactID, , "")
        strContactID = fxnChangeDelimiter(strContactID, "'", "")
        ' Change double-spaces to single spaces
        strTemp = fxnChangeDelimiter(strTemp, "  ", " ")
        ' Change any spaces to underbars
        strContactID = fxnChangeDelimiter(strContactID, " ", "_")
        If strContactID <> "" Then
            If MsgBox("The contact ID will be: " & vbCrLf & strContactID, _
                vbOKCancel, "Confirm new contact ID") = vbOK Then
                Me.txtContactID = strContactID
            Else
                cmdUndo_Click
            End If
        Else
            cmdUndo_Click
        End If
    End If

Exit_Procedure:
    Exit Function

Err_Handler:
    MsgBox "Error #" & Err.Number & ": " & Err.Description, vbCritical, _
        "Error encountered (#" & Err.Number & " - fxnBuildID)"
    Resume Exit_Procedure

End Function

' ---------------------------------
' FUNCTION:     fxnValidate
' Description:  Validate the record prior to saving, closing or moving to another record
' Parameters:   none
' Returns:      True if the record passes validation rules, or False
' Throws:       none
' References:   none
' Source/date:  John R. Boetsch, May 25, 2006
' Revisions:    <name, date, desc - add lines as you go>
' ---------------------------------
Function fxnValidate() As Boolean
    On Error GoTo Err_Handler

    Dim strErrorMsg As String

    fxnValidate = False     ' Default unless successful

    strErrorMsg = "There is already a contact with the same ID." & _
        "If this record is for a different person with a similar name, " & _
        "either add a middle initial if there is none, or " & _
        "make a minor modification to the ID below.  Otherwise UNDO edits."

    ' Make sure the information is valid before updating the record
    If Me.Dirty = True Then
        ' If information for a new contact has been entered,
        '  confirm that the critical data elements have been completed before saving
        If IsNull(Me.Last_name) Then
            MsgBox "Fill in the last name", vbOKOnly, "Validation error"
            Me.txtLastName.SetFocus
            GoTo Exit_Procedure
        ElseIf IsNull(Me.First_name) Then
            MsgBox "Fill in the first name", vbOKOnly, "Validation error"
            Me.txtFirstName.SetFocus
            GoTo Exit_Procedure
        ElseIf IsNull(Me.Organization) Then
            MsgBox "Fill in the employer/organization of the contact", vbOKOnly, _
                "Validation error"
            Me.cmbOrganization.SetFocus
            GoTo Exit_Procedure
        ' Make sure that the record is not a duplicate prior to saving
        ElseIf Me.NewRecord And DCount("*", "tlu_Project_Crew", "Contact_ID =""" _
            & Me!txtContactID & """") > 0 Then
            MsgBox strErrorMsg, vbOKOnly, "Duplicate record found"
            Me.txtContactID.Locked = False
            Me.txtContactID.SetFocus
            GoTo Exit_Procedure
        End If
    End If

    fxnValidate = True

Exit_Procedure:
    Exit Function

Err_Handler:
    Select Case Err.Number
      Case 3  ' only a problem in ADP files with SQL Server back end
        MsgBox "Error #" & Err.Number & ": There is a SQL syntax error in the " & _
            vbCrLf & "DCount function due to a quotation mark in the key value." & _
            vbCrLf & "Please undo edits and notify the database administrator.", _
            vbCritical, "Unable to look for duplicate IDs (fxnValidate)"
      Case Else
        MsgBox "Error #" & Err.Number & ": " & Err.Description, vbCritical, _
            "Error encountered (#" & Err.Number & " - fxnValidate)"
    End Select
    Resume Exit_Procedure

End Function