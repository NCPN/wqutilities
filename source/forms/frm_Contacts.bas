' FORM NAME:    frm_Contacts
' Description:  Standard form for viewing and editing contact information
' Data source:  tlu_Project_Crew
' Data access:  no edits, additions, or deletions unless properties are changed
'               (see fxnFormDefaults)
' Pages:        none
' Functions:    fxnBuildID, fxnFormDefaults, fxnValidate
' References:   fxnChangeDelimiter, fxnSwitchboardIsOpen, fxnTableExists
' Source/date:  John R. Boetsch, 2002
' Adapted:      Bonnie Campbell, June 2014
' Revisions:    JRB, 5/25/2006 - documentation, changed validation, and combined
'                   what was previously in a subform into a single form
'               JRB, 6/8/2007 - updated fxnBuildID and fxnFormDefaults
'               JRB, 4/21/2008 - revised input masks and formats for telephone numbers
'               JRB, 5/14/2008 - removed txtNPS_login
'               JRB, 5/21/2008 - updated title bar to Contact Information Form
'               JRB, 6/27/2008 - standardization with other project dbs; updated cmbContact SQL
'               JRB, 9/5/2008 - added error trap on cmdClose
'               JRB, 10/14/2008 - updated Form_Open and added Form_Dirty to prevent data
'                   changes in "read only" mode; updated save only on dirty record;
'                   updated gvarRefForm/Ctl error code in cmdClose
'               JRB, 7/9/2009 - replaced fxnTrimSpaces with Trim(); added ref to fxnTableExists
'               BLC, 6/12/2014 - Revised to use TempVars.Item("UserAccessLevel") vs. cAppMode
' =================================

Dim bNoRecords As Boolean   ' If no records in the recordset

' ---------------------------------
' SUB:     Form_Open
' Description: Opens sub form & sets controls based on UserAccessLevel
' Parameters:   -
' Returns:      -
' Throws:       -
' References:   -
' Source/date:  Created John R. Boetsch, September 2008
' Adapted:      Bonnie Campbell, June 2014
' Revisions:    BLC, 6/12/2014 - Revised to use TempVars.Item("UserAccessLevel") vs. cAppMode
' ---------------------------------
Private Sub Form_Open(Cancel As Integer)
    On Error GoTo Err_Handler

    ' Count the records in the recordset to determine the form settings
    If DCount("*", "tlu_Project_Crew") = 0 Then
        ' If no records, set view to new
        bNoRecords = True
        fxnFormDefaults ("new")
    ElseIf Me.FilterOn Then
    ' If the form is filtered, set the filter ctl to the value sent by the referring form
        Me.cmbContact = Me.Contact_ID
        Me.optgFilterContacts = 1
        optgFilterContacts_AfterUpdate
    ElseIf Me.OpenArgs = "new" Then
        fxnFormDefaults ("new")
    Else
        fxnFormDefaults ("view")
    End If

    If fxnSwitchboardIsOpen Then
        Select Case TempVars.item("UserAccessLevel")
          Case "read only"
            ' Disable edit controls for "read only" application status
            Me.cmdEdit.Enabled = False
            Me.cmdNew.Enabled = False
            Me.cmdUndo.Enabled = False
            Me.cmdSubmit.Enabled = False
            GoTo Exit_Procedure
        End Select
    End If

Exit_Procedure:
    Exit Sub

Err_Handler:
    MsgBox "Error #" & Err.Number & ": " & Err.Description, vbCritical
    Resume Exit_Procedure

End Sub


' ---------------------------------
' SUB:     Form_Current
' Description:  Updates form records display
' Parameters:   -
' Returns:      -
' Throws:       -
' References:   -
' Source/date:  Created John R. Boetsch, September 2008
' Adapted:      Bonnie Campbell, June 2014
' Revisions:    BLC, 6/12/2014 - XX
' ---------------------------------
Private Sub Form_Current()
    On Error GoTo Err_Handler

    ' On moving to a different record, turn off the filter and update the
    '   contact selector
    If Me.optgFilterContacts = 0 Then
        Me.FilterOn = False
        Me.cmbContact.Enabled = False
        Me.cmbContact = Me.txtContactID
    ElseIf Me.optgFilterContacts = 1 And Me.cmbContact.Enabled Then
        Me.cmbContact.SetFocus
    End If

Exit_Procedure:
    Exit Sub

Err_Handler:
    MsgBox "Error #" & Err.Number & ": " & Err.Description, vbCritical
    Resume Exit_Procedure

End Sub

' ---------------------------------
' SUB:     Form_Dirty
' Description:  Cancels changes for read only users
' Parameters:   -
' Returns:      -
' Throws:       -
' References:   -
' Source/date:  Created John R. Boetsch, September 2008
' Adapted:      Bonnie Campbell, June 2014
' Revisions:    BLC, 6/19/2014 - Replaced cAppMode with TempVars.Item("UserAccessLevel")
' ---------------------------------
Private Sub Form_Dirty(Cancel As Integer)
    On Error GoTo Err_Handler

    ' Note: this event is ignored on inserting a new record if BeforeInsert code exists

    ' Cancel data changes if the application is set to read only
    If fxnSwitchboardIsOpen Then
        If TempVars.Items("UserAccessLevel") = "read only" Then DoCmd.CancelEvent
    End If

Exit_Procedure:
    Exit Sub

Err_Handler:
    MsgBox "Error #" & Err.Number & ": " & Err.Description, vbCritical
    Resume Exit_Procedure

End Sub

' ---------------------------------
' SUB:     Form_BeforeUpdate
' Description:  Data validation
' Parameters:   -
' Returns:      -
' Throws:       -
' References:   -
' Source/date:  Created John R. Boetsch, September 2008
' Adapted:      Bonnie Campbell, June 2014
' Revisions:    BLC, 7/29/2014 - updated to use TempVars.Item("User") vs. cUser,
'                                similarly TempVars.Item("Project") vs. cProject
' ---------------------------------
Private Sub Form_BeforeUpdate(Cancel As Integer)
    On Error GoTo Err_Handler

    ' Validate the record and cancel updates if not valid
    If fxnValidate = False Then
        DoCmd.CancelEvent
        GoTo Exit_Procedure
    End If

    ' Prior to saving, include a timestamp for edits
    If Me.NewRecord = False Then Me.txtUpdated = Now()
    ' Add the current user name to updated by
    If fxnSwitchboardIsOpen Then
        If IsNull(TempVars.item("User")) = False Then
            Me.txtUpdatedBy = TempVars.item("User")
        Else
            Me.txtUpdatedBy = Environ("Username")
        End If
    Else
        Me.txtUpdatedBy = Environ("Username")
    End If
    ' Update the project code
    If IsNull(Me.txtProjectCode) Then
        If fxnSwitchboardIsOpen Then
            Me.txtProjectCode = TempVars.item("Project")
        ElseIf fxnTableExists("tsys_App_Defaults") Then
            Me.txtProjectCode = DLookup("[Project]", "tsys_App_Defaults")
        Else
            Me.txtProjectCode = "Unknown"
        End If
    End If

Exit_Procedure:
    Exit Sub

Err_Handler:
    MsgBox "Error #" & Err.Number & ": " & Err.Description, vbCritical
    Resume Exit_Procedure

End Sub

